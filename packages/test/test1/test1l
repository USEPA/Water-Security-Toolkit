#!/bin/csh
##
## Test1 test script
##

setenv bindir `bindirLocation`
setenv moddir `moddirLocation`
setenv python `pythonLocation`

if (! -e test1.tso) then
   $bindir/tevasim --tsg test1.tsg --tsi test1.tsi --tso test1.tso Net3.inp Net3.out
endif
if (! -e test1_mc.impact) or (! -e test1_vc.impact) then
   $bindir/tso2Impact --mc --vc test1 test1.tso
endif

$python $bindir/sp --path=$bindir --path=$moddir --network=test1 --objective=mc --ub=ns,5 --ub=vc,35000 --solver=pico --gamma=0.01
