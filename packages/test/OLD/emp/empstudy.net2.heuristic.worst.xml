<experimental-study name="teva-sp">
  <tags>
    <tag>smoke</tag>
    <tag>nightly</tag>
  </tags>


  <!-- assumes that 
       runexp empstudy.net2.gentso.xml
       runexp empstudy.net2.genimpact.xml
       have been run -->

  <experiment name="worst">
    <factors>
      <factor name="goals">
        <level>mc</level>
      </factor>
      <factor name="measure">
        <level>worst</level>
      </factor>
      <factor name="impact_prefix">
        <level>\-\-network=net2sanTEVA</level>
      </factor>
      <factor name="sensors">
        <level>-\-ub=ns,20</level>
      </factor>
      <factor name="samples">
        <level>-\-numsamples=1</level>
      </factor>
      <factor name="solver">
        <level>-\-solver=heuristic</level>
      </factor>
    </factors>
    <controls>
       <executable>exp.teva-sp.pl -sp-simple</executable>
    </controls>
  </experiment>

</experimental-study>
